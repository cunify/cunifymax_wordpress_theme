<html lang="en">
    <head>
        
        <!-- Title -->
        <title>
            {% if wp_title %}
                {{ wp_title }} - {{ site.name }}
            {% else %}
                {{ site.name }}
            {% endif %}
        </title>

        <!-- Required Meta Tags Always Come First -->
        <meta charset="{{ site.charset }}">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta http-equiv="x-ua-compatible" content="ie=edge">

        <link rel="author" href="{{ site.theme.link }}/humans.txt" />
        <link rel="pingback" href="{{ site.pingback_url }}" />

        <meta name="description" content="{{ site.description }}">

        <!-- Favicon -->
        <link rel="shortcut icon" href="../../../favicon.ico">
        <!-- Google Fonts -->
        <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans%3A400%2C300%2C500%2C600%2C700%7CPlayfair+Display%7CRoboto%7CRaleway%7CSpectral%7CRubik">
        <!-- CSS Global Compulsory -->
        <link rel="stylesheet" href="{{ site.url }}/wp-content/themes/cunifymax/assets/vendor/bootstrap/bootstrap.min.css">
        <!-- CSS Global Icons -->
        <link rel="stylesheet" href="{{ site.url }}/wp-content/themes/cunifymax/assets/vendor/icon-awesome/css/font-awesome.min.css">
        <link rel="stylesheet" href="{{ site.url }}/wp-content/themes/cunifymax/assets/vendor/icon-hs/style.css">
        <link rel="stylesheet" href="{{ site.url }}/wp-content/themes/cunifymax/assets/vendor/slick-carousel/slick/slick.css">
        <link rel="stylesheet" href="{{ site.url }}/wp-content/themes/cunifymax/assets/vendor/animate.css">
        <link rel="stylesheet" href="{{ site.url }}/wp-content/themes/cunifymax/assets/vendor/chosen/chosen.css">

        <!-- CSS Customization -->
        <link rel="stylesheet" href="{{ site.url }}/wp-content/themes/cunifymax/assets/css/custom.css">

